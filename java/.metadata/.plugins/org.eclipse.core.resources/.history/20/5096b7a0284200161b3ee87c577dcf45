import java.util.*;

public class DotProduct {
    private static long minDotProduct(int[] a, int[] b) {
        //write your code here
    	int[]b1=new int[b.length];
    	int[]a1=new int[a.length];
    	for(int i=0;i<b.length;i++){
    		a1[i]=a[i];
    		b1[i]=b[i];
    	}
        long result = 0;
        Arrays.sort(b);
        Arrays.sort(a);
        int max=0,min=0;
        int size=a.length;
        for(int i=0;i<size;i++){
        	result+=a[size-1-i]*b[i];
        }
        /*for (int i = 0; i < a.length; i++) {
            max=max(a1);
            min=min(b1);
            a1[max]=-2147483648;
            b1[min]=2147483647;
        	result += a[max] * b[min];
        	
        }*/
        return result;
    }
